{
	"manifest_version": 3,
	"name": "LeetCode Code Extractor React",
	"version": "1.0",
	"permissions": ["scripting", "storage", "activeTab", "tabs"],
	"host_permissions": [
	  "https://leetcode.com/*",
	  "https://algolistapi.vercel.app/*"
	],
	"action": {
	  "default_popup": "index.html"
	},
	"content_scripts": [
	  {
		"matches": ["https://leetcode.com/*"],
		"js": ["content.js"]
	  }
	]
  }